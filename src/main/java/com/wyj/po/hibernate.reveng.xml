<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="db_blog" match-name="article"/>
  <table-filter match-catalog="db_blog" match-name="bloginfo"/>
  <table-filter match-catalog="db_blog" match-name="critique"/>
  <table-filter match-catalog="db_blog" match-name="hits"/>
  <table-filter match-catalog="db_blog" match-name="user"/>
</hibernate-reverse-engineering>